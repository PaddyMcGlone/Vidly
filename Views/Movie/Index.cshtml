@model System.Collections.Generic.List<Vidly.Models.Movie>

@{
    ViewBag.Title = "Movies";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Movies</th>
        </tr>
    </thead>

    @foreach (var movie in Model)
    {
        <tr>
            <td>@movie.Name</td>
        </tr>
    }
</table>